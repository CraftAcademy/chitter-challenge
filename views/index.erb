<main>
  <div class="row">
    <div class="large-12 columns">
      <div class="panel">
        <% @peeps.reverse_each do |peep| %>
            <div class="row">
              <div class="large-8 columns">
                <div class="callout panel">
                  <%= peep[:message] %></p>
                  <p>
                    <small>
                      <b>Created:</b> <%= peep[:created_at] %> <b>Posted by</b>: <%= peep[:created_by_name] %>
                      <b>username:</b> <i><%= peep[:created_by] %></i>
                    </small>
                  </p>
                </div>
              </div>
            </div>
        <% end %>
      </div>
    </div>
</main>
